div\">\n      <form\n        v-if=\"tabIndex == 1\"\n        @submit=\"search($event)\"\n      >\n        <van-search\n          v-model=\"searchObj.companyName\"\n          placeholder=\"请输入客户名称\"\n        />\n      </form>\n      <form\n        v-if=\"tabIndex == 2\"\n        @submit=\"search($event)\"\n      >\n        <van-search\n          v-model=\"searchObj.operatorName\"\n          placeholder=\"请输入操作人名称\"\n        />\n      </form>\n      <form\n        v-if=\"isSearch\"\n        @submit=\"search($event)\"\n      >\n        <van-search\n          v-if=\"tabIndex == 3\"\n          v-model=\"searchObj.fileName\"\n          placeholder=\"请输入附件名称\"\n        />\n        <van-search\n          v-else\n          v-model=\"searchObj.rentBackName\"\n          placeholder=\"请输入归还名称\"\n        />\n      </form>\n      <div\n        v-show=\"!notShowSearch\"\n        class=\"search_btn_div\"\n        @click=\"searchToggle()\"\n      >\n        <span>筛选</span>\n        <span class=\"ionicFont iconfont iconshaixuan\" />\n      </div>\n      <div\n        v-if=\"isSort\"\n        class=\"search_btn_div\"\n        @click=\"runSort\"\n      >\n        <span>排序</span>\n        <span class=\"ionicFont  iconfont  iconpaixu\" />\n      </div>\n      <div class=\"search_btn_div1\">\n        <van-button\n          class=\"search-btn\"\n          size=\"small\"\n          type=\"primary\"\n          @click=\"search($event)\"\n        >\n          搜索\n        </van-button>\n      </div>\n    </div>\n    <!-- 筛选框 -->\n    <!-- <transition name=\"van-slide-down\"> -->\n    <div\n      v-if=\"visible\"\n      class=\"search_box\"\n    >\n      <div class=\"container\">\n        <div\n          v-if=\"tabIndex == 1\"\n          class=\"search-item\"\n        >\n          <div class=\"left\">\n            行业名称\n          </div>\n          <div class=\"right\">\n            <input\n              v-model=\"searchObj.companyIndustry\"\n              placeholder=\"请输入行业名称信息\"\n              type=\"text\"\n            >\n          </div>\n        </div>\n        <div\n          v-if=\"tabIndex == 2\"\n          class=\"search-item\"\n        >\n          <div class=\"left\">\n            项目名称\n          </div>\n          <div class=\"right\">\n            <input\n              v-model=\"searchObj.projectName\"\n              placeholder=\"请输入项目名称\"\n              type=\"text\"\n            >\n          </div>\n        </div>\n        <div class=\"search-item\">\n          <div class=\"left\">\n            {{ search_item2_text }}\n          </div>\n          <div class=\"right\">\n            <select\n              v-if=\"tabIndex == 1\"\n              v-model=\"searchObj.companyCustomerType\"\n            >\n              <option value=\"\">\n                请选择\n              </option>\n              <option value=\"意向客户\">\n                意向客户\n              </option>\n              <option value=\"潜在客户\">\n                潜在客户\n              </option>\n              <option value=\"一般客户\">\n                一般客户\n              </option>\n            </select>\n            <select\n              v-if=\"tabIndex == 2\"\n              v-model=\"searchObj.actionType\"\n            >\n              <option :value=\"null\">\n                请选择\n              </option>\n              <option value=\"报价\">\n                报价\n              </option>\n              <option value=\"尽调\">\n                尽调\n              </option>\n              <option value=\"上会\">\n                上会\n              </option>\n              <option value=\"签约\">\n                签约\n              </option>\n              <option value=\"投放\">\n                投放\n              </option>\n              <option value=\"贷后\">\n                贷后\n              </option>\n            </select>\n            <select\n              v-if=\"tabIndex == 3\"\n              v-model=\"searchObj.rentBackType\"\n              v-placeholder=\"'请选择类型'\"\n            >          \n        
dProject',res)\\n        this.EventsData = res.information\\n      })\\n    },\\n    formatter(type, value) {\\n      if (type === 'year') {\\n        return `${value}年`\\n      } if (type === 'month') {\\n        return `${value}月`\\n      }\\n      return `${value}日`\\n    },\\n    startTimeClick() {\\n      this.selectStartTime = true\\n    },\\n    startconfirm(e) {\\n      // 选开始时间\\n      if (isNull(e)) {\\n        this.selectStartTime = false\\n      } else {\\n        const time = formatDate(new Date(e), 'yyyy-MM-dd', 'special')\\n        const timeStr = time.split('-')\\n        this.EventsData.tuneUpTime = `${timeStr[0]}-${timeStr[1]}-${timeStr[2]}`\\n        this.selectStartTime = false\\n      }\\n    },\\n    callBack(){\\n      this.$emit('addCall',this.EventsData)\\n    },\\n    getProjectList() {\\n      const params = {\\n        menuType: 1,\\n        size:1000\\n      }\\n      projectList(params).then((res) => {\\n        // console.log('projectList', res)\\n        this.pgList = res.data\\n        if(res.data.length>0){\\n          if(this.EventsData.projectName){\\n            return\\n          }\\n          this.EventsData.projectName = res.data[0].information.projectName\\n        }\\n        \\n      })\\n    },\\n    clickAddEvents() {\\n      const EventsData = {}\\n      this.pgList.map(d => {\\n        this.EventsData.projectId = d.information.projectName == this.EventsData.projectName ? d.id : this.EventsData.projectId\\n      })\\n      EventsData.information = JSON.stringify(this.EventsData) \\n      EventsData.menuType = 3\\n      EventsData.projectId = this.EventsData.projectId\\n      EventsData.visitDTO = this.visitDTO\\n      // console.log('pgList',pgList)\\n      // console.log('EventsData',EventsData)\\n      let txts = {                                \\n        tuneUpMode:'尽调方式',        \\n      }       \\n      if (!this.$checkFrom(this.EventsData,txts)) {\\n        return\\n      }\\n      addProject(EventsData).then(res=